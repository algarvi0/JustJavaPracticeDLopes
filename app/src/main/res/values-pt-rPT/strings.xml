<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
    <string name="app_name">JustJavaPracticeDLopes</string>
    <!-- CÓDIGO DE DLOPES-->
    <string name="order_your_coffee">Order Your Coffee</string>
    <string name="string_price">order summary</string>
    <string name="button_order">Order</string>
    <string name="button_minus" translatable="false">-</string>
    <string name="button_plus" translatable="false">+</string>
    <string name="instructions">Press - to decrease or + to increase then press ORDER !"</string>
    <string name="less_then_one">Pode pedir no mínimo 1 café  !</string>
    <string name="name">Nome</string>
    <string name="toppings">Coberturas</string>
    <string name="whipped_cream">Natas</string>
    <string name="chocolate">Chocolate</string>
    <string name="quantity">Quantidade</string>
    <string name="initial_quantity_value" translatable="false">2</string>
    <string name="maximum_coffes">Pode pedir um máximo de 100 cafés !</string>

    <string name="name_error">Por favor digite o seu nome</string>
    <string name="IntegralEmailAddress" translatable="false">algarvi0@gmail.com</string>
    <!--
      Name for the order summary. It will be shown in the format of "Name: Amy" where Amy is the
      user's name. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_name">Nome: <xliff:g id="name" example="Amy">%s</xliff:g></string>

    <!--
      Whipped cream topping for the order summary. It will be shown in the format of
      "Add whipped cream? true" or "Add whipped cream? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_whipped_cream">Adicionar Natas? <xliff:g id="stringAddChocolate" example="true">%s</xliff:g></string>

    <!--
      Chocolate topping for the order summary. It will be shown in the format of
      "Add chocolate? true" or "Add chocolate? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_chocolate">Adicionar chocolate? <xliff:g id="stringAddChocolate" example="true">%s</xliff:g></string>

    <!--
      Quantity of coffee cups for the order summary. It will be shown in the format of
      "Quantity: 2", where 2 is the number of cups ordered. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_quantity">Quantidade: <xliff:g example="2" id="quantity">%d</xliff:g></string>

    <!--
      Total price for the order summary. It will be shown in the format of
      "Total: $10" where $10 is the price. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_price">Total: <xliff:g id="price" example="$10">%s</xliff:g></string>

    <!-- Thank you message for the order summary. [CHAR LIMIT=NONE] -->
    <string name="thank_you">Obrigado !</string>

    <!--
      Subject line for the order summary email. It will be in the format of
      "Just Java order for Amy" where Amy is the user's name. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_email_subject">Só Java - Pedido para <xliff:g example="Amy" id="name">%s</xliff:g></string>
    <string name="insert_your_name">Digite o seu nome</string>
    <string name="_1">1</string>
    <string name="order">Pedido</string>
    <string name="minus">-</string>
    <string name="_0">0</string>

    <string name="no">Não</string>
    <string name="yes">Sim</string>


</resources>
